<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.upowarz.aeroplanechess">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="com.upowarz.aeroplanechess.PersonalPage"></activity>
        <activity
            android:name="com.upowarz.aeroplanechess.CreateRoomPage"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name="com.upowarz.aeroplanechess.RoomHallPage"
            android:screenOrientation="landscape"></activity>
        <activity android:name="com.upowarz.aeroplanechess.LoadingPage" />
        <activity
            android:name="com.upowarz.aeroplanechess.MutilplayerPage"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.upowarz.aeroplanechess.StartPage"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.upowarz.aeroplanechess.MenuPage"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.upowarz.aeroplanechess.LoginPage"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.upowarz.aeroplanechess.GameProcessPage"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.upowarz.aeroplanechess.MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>